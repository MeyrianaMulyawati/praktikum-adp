program dor;
uses crt;
var
  i,j,k,l :Integer;
begin
clrscr;
  j:=0;
  for k := 0 to 9 do
    begin
  for i := 1 to 10 do
    begin
        l := 10*k + i;
      if (l mod 3 = 0) or (l mod 5 = 0) then
        begin
        write('DOR ');
        j:=j+1;
        end
      else
      write(l, ' ');
    end;
    Writeln;
    end;
    Writeln('Jumlah DOR : ',j);
    readln;
end.
